{
  "name": "makys/my-api",
  "description": "A simple API using PHP",
  "version": "1.0.0",
  "autoload": {
    "psr-4": {
      "App\\": "app/"

    }
  },
  "require": {
    "php": "^7.4",
    "ext-fileinfo": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "psr/container": "^2.0",
    "tracy/tracy": "^v2.7.7"
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "symfony/flex": true
    },
    "process-timeout": 0,
    "sort-packages": true
  }
}
